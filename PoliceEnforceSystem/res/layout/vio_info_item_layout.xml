<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_marginLeft="@dimen/margin_padding10"
    android:layout_marginRight="@dimen/margin_padding10"
    android:background="@drawable/list_item_select"
    android:gravity="center_vertical"
    android:orientation="vertical"
    android:paddingBottom="@dimen/margin_padding2"
    android:paddingTop="@dimen/margin_padding2" >

    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/margin_padding5"
        android:orientation="horizontal" >

        <TextView
            android:id="@+id/textView01"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/margin_padding10"
            android:layout_weight="1"
            android:singleLine="true"
            android:text="AA"
            android:textColor="@color/color333333"
            android:textSize="@dimen/textSize16" />

        <TextView
            android:id="@+id/textView02"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:singleLine="true"
            android:text="SS"
            android:textColor="@color/color666666"
            android:textSize="@dimen/textSize16" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/margin_padding5"
        android:orientation="horizontal" >

        <TextView
            android:id="@+id/textView03"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/margin_padding10"
            android:layout_weight="1"
            android:singleLine="true"
            android:text="SS"
            android:textColor="@color/color666666"
            android:textSize="@dimen/textSize16" />

        <TextView
            android:id="@+id/textView04"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:singleLine="true"
            android:text="SS"
            android:textColor="@color/color666666"
            android:textSize="@dimen/textSize16" />
    </LinearLayout>

    <TextView
        android:id="@+id/textView05"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="@dimen/margin_padding10"
        android:layout_marginTop="@dimen/margin_padding5"
        android:singleLine="true"
        android:text="SS"
        android:textColor="@color/color333333"
        android:textSize="@dimen/textSize16" />

    <TextView
        android:id="@+id/textView06"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="@dimen/margin_padding10"
        android:layout_marginTop="@dimen/margin_padding5"
        android:singleLine="true"
        android:text="SS"
        android:textColor="@color/color666666"
        android:textSize="@dimen/textSize16" />

    <TextView
        android:id="@+id/textView07"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="@dimen/margin_padding10"
        android:layout_marginTop="@dimen/margin_padding5"
        android:singleLine="true"
        android:text="SS"
        android:textColor="@color/color333333"
        android:textSize="@dimen/textSize16" />

    <TextView
        android:id="@+id/textView08"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="@dimen/margin_padding10"
        android:layout_marginTop="@dimen/margin_padding5"
        android:text="SS"
        android:textColor="@color/color333333"
        android:textSize="@dimen/textSize16" />

</LinearLayout>